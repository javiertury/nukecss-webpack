{
  "name": "nukecss-webpack",
  "version": "0.0.0",
  "description": "Eliminates unused css from your webpack bundle.",
  "main": "./lib/index.js",
  "scripts": {
    "start": "webpack --config test/fixtures/webpack.config.js",
    "start:debug": "node --inspect --debug-brk ./node_modules/.bin/webpack -p --config test/fixtures/webpack.config.js",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "mocha --reporter spec --require test/bootstrap 'test/**/*.test.js'",
    "test:lint": "xo './lib/**/*.js' './test/**/*.test.js'",
    "test:coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --require test/bootstrap 'test/**/*.test.js'",
    "test:watch": "mocha --watch --reporter dot --require test/bootstrap 'test/**/*.test.js'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickhulce/nukecss-webpack.git"
  },
  "author": "Patrick Hulce <patrick.hulce@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/patrickhulce/nukecss-webpack/issues"
  },
  "homepage": "https://github.com/patrickhulce/nukecss-webpack#readme",
  "xo": {
    "env": [
      "node",
      "mocha"
    ],
    "global": [
      "expect"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "esprima": "^3.1.3",
    "lodash": "^4.17.4",
    "nukecss": "^1.0.0",
    "webpack-sources": "^0.1.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "extract-text-webpack-plugin": "v2.0.0-rc.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.0",
    "semantic-release": "^6.3.2",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "xo": "github:patrickhulce/xo#master"
  }
}
